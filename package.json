    "@vercel/analytics": "^0.1.8",
    "autoprefixer": "^10.4.12",
    "clsx": "^1.2.1",
    "focus-visible": "^5.2.0",
    "js-yaml": "^4.1.0",
    "next": "13.1.6",
    "postcss": "^8.4.21",
    "postcss-focus-visible": "^8.0.1",
    "postcss-import": "^15.1.0",
